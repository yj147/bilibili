# Backend URL consumed by Next.js API proxy route
# Default works for most local setups.
BACKEND_INTERNAL_URL=http://localhost:8000

# If localhost routing is special in your environment (e.g. some WSL setups), use:
# BACKEND_INTERNAL_URL=http://127.0.0.1:8000

# Frontend API settings
NEXT_PUBLIC_API_BASE=/api
NEXT_PUBLIC_API_KEY=
